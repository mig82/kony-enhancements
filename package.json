{
  "name": "kronin",
  "version": "2.0.0-rc5",
  "description": "A collection of useful extensions for the Kony SDK.",
  "main": "bin/kronin.js",
  "scripts": {
    "prepare": "browserify src/main.js -o bin/kronin.js",
    "copypack": "mkdir -p tests/workspace/KroninTest1/modules/node_modules/kronin && cp package.json tests/workspace/KroninTest1/modules/node_modules/kronin",
    "watch": "watchify src/main.js -o tests/workspace/KroninTest1/modules/node_modules/kronin/bin/kronin.js",
    "pretest": "npm run copypack && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mig82/kony-sdk-extensions.git"
  },
  "keywords": [
    "Kony",
    "utilities",
    "extensions"
  ],
  "author": "Miguelangel Fernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mig82/kony-sdk-extensions/issues"
  },
  "homepage": "https://github.com/mig82/kony-sdk-extensions#readme",
  "devDependencies": {
    "browserify": "^16.3.0",
    "tinyify": "^2.5.1",
    "watchify": "^3.11.1"
  },
  "dependencies": {}
}
